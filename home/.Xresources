Xft.dpi: DISPLAY_DPI
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintful
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

Xterm.Geometry: 200x200+0+0

URxvt.font: xft:Monospace:size=10
URxvt.internalBorder: 50
URxvt.scrollBar: false
URxvt.lineSpace: 2

URxvt.perl-ext: default,matcher,keyboard-select
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.keysym.C-q: matcher:select
URxvt.keysym.M-q: matcher:list
URxvt.matcher.pattern.2: \\B(/\\S+?):(\\d+)(?=:|$)
URxvt.matcher.launcher.2: vim +$2 $1
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
! Clear scroll-back on Contrl+L
URxvt.keysym.Control-l: command:\033c

